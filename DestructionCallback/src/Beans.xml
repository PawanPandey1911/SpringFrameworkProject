<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="hw1" class="com.learnerslife.HelloWorld" >
		<property name="message" value="Hello World!" />
	</bean>
	<bean id="hw2" class="com.learnerslife.HelloWorld" >
		<property name="message" value="hi World!" />
	</bean>
	<bean id="hw3" class="com.learnerslife.HelloWorld" destroy-method="beandestroy">
		<property name="message" value="Hello World!" />
	</bean>
	<bean id="hw4" class="com.learnerslife.HelloWorld" destroy-method="beandestroy">
		<property name="message" value="hi World!" />
	</bean>
</beans>