<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	 default-lazy-init="false">
	 
	<bean id="hw1" class="com.learnerslife.HelloWorld" init-method="getbeanid" destroy-method="destroybean">
		<property name="message" value="Hello World" />
		<property name="beanid" value="hw 1" />
	</bean>
	<bean id="hw2" class="com.learnerslife.HelloWorld" init-method="getbeanid" destroy-method="destroybean">
		<property name="message" value="hi World!" />
		<property name="beanid" value="hw 2" />
	</bean>
	
</beans>